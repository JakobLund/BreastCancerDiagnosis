@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<script type="text/javascript">
    var FileLoc = "@ViewBag.ImageId";
</script>
<h2 style="margin-bottom: 2em">Vent venligst mens Mark analyserer billedet..</h2>
<div class="spinner-border center" role="status" style="animation-duration: 2.6s; width: 300px; height: 300px;">
    <span class="sr-only">Loading...</span>
</div>

<h1 class="result display-3" align="center"></h1>
<div style="margin-top:10px" class="progress">
